{
  "expo": {
    "name": "docTRA",
    "slug": "docTRA",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.7.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ios.docTRA",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.813152005106-pkpr0bgi2fnh86vmjth9d9qscclqh783"
        }
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.docTRA",
      "googleServicesFile": "./google-services.json"
    },
    "description": "Doctor training reinforcement and assessment application",
    "githubUrl": "https://github.com/gradytucker/docTRA.git"
  }
}
